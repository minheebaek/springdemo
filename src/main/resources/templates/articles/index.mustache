{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">ID</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#articleList}} <!--"articleList",articleEntityList 컨트롤러에서 설정한 이름과 일치해야 쓸 수 있다-->
        <tr>
            <th>{{id}}</th>
            <td><a href="/articles/{{id}}">{{title}}</td>
            <td>{{content}}</td>
        </tr>
    {{/articleList}}
    </tbody>
</table>

<a href="/articles/new">New Articles</a>

{{>layouts/footer}}